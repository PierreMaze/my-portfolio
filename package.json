{
  "name": "portfolio-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "http://pierremaze.github.io/my-portfolio",
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "scripts": {
    "dev": "vite",
    "reset": "pnpm run clean && pnpm i && pnpm run dev",
    "preload": "pnpm i && pnpm run dev",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fix": "prettier --write .",
    "clean": "rm -rf node_modules pnpm-lock.yaml package-lock.json dist",
    "deploy": "gh-pages -d dist -f"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tailwindcss/vite": "^4.1.18",
    "framer-motion": "^12.23.22",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "vite": "^7.2.7"
  },
  "keywords": [
    "Dev",
    "Portfolio",
    "React",
    "TailwindCSS",
    "Framer Motion"
  ],
  "author": "Pierre MAZELAYGUE",
  "license": "ISC",
  "description": "Project portfolio with React, TailwindCSS and modern technologies."
}